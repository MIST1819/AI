#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
#include <string.h>

using namespace std;

int coprime(int a, int b)
{
    if ( __gcd(a, b) == 1)
        return 1;
    else
        return 0;
}


int main()
{
    int t, counter,i,j;
    char letter = 'a';
    char str[1000],o;
    int count[26];
    int cop;

    //cout<<"enter t";
    cin>>t;

    while(t--)
    {


        //cout<<"Enter string:";

        cin>>str;

        for (i=0;i<26;i++)
            count[i]=0;

        for(i=0;i<26;i++)
        {
            counter=0;

            for(j=0;j<strlen(str);j++)
                if (str[j]==letter+i)
                    counter++;

            count[i]=counter;
        }

        cop=0;

        for (i=0;i<26;i++)
            for (j=i+1;j<26;j++)
                cop=cop+coprime(count[i],count[j]);

        cout<<cop<<endl;
    }

    return 0;
}
